CREATE TABLE NBAPlayer (
    player_id NOT NULL UNIQUE PRIMARY KEY,
    first_name TEXT NOT NULL,
    last_name TEXT NOT NULL,
    image TEXT NOT NULL
    );

selectAll:
SELECT * FROM NBAPlayer;

insertOrReplace:
INSERT OR REPLACE INTO NBAPlayer(
    player_id,
    first_name,
    last_name,
    image
)
VALUES(?,?,?,?);

selectById:
SELECT * FROM NBAPlayer WHERE player_id = ?;


